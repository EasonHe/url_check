    {
      "id": 203,
      "type": "table",
      "title": "Task Overview / 任务总览",
      "description": "第一列固定 task_name，第二列任务状态，后续为告警项。ALERT=橙色，OK=绿色，NoData=灰色。",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "x": 0,
        "y": 4,
        "w": 24,
        "h": 14
      },
      "targets": [
        {
          "refId": "A",
          "instant": true,
          "format": "table",
          "expr": "sum by (task_name) (clamp_max(url_check_status_code_alert + url_check_timeout_alert + url_check_content_alert + url_check_json_path_alert + url_check_delay_alert + url_check_ssl_expiry_alert, 1))"
        },
        {
          "refId": "B",
          "instant": true,
          "format": "table",
          "expr": "url_check_status_code_alert{task_name=~\".*\"}"
        },
        {
          "refId": "C",
          "instant": true,
          "format": "table",
          "expr": "url_check_timeout_alert{task_name=~\".*\"}"
        },
        {
          "refId": "D",
          "instant": true,
          "format": "table",
          "expr": "url_check_content_alert{task_name=~\".*\"}"
        },
        {
          "refId": "E",
          "instant": true,
          "format": "table",
          "expr": "url_check_json_path_alert{task_name=~\".*\"}"
        },
        {
          "refId": "F",
          "instant": true,
          "format": "table",
          "expr": "url_check_delay_alert{task_name=~\".*\"}"
        },
        {
          "refId": "G",
          "instant": true,
          "format": "table",
          "expr": "url_check_ssl_expiry_alert{task_name=~\".*\"}"
        }
      ],
      "transformations": [
        {
          "id": "labelsToFields",
          "options": {
            "mode": "columns"
          }
        },
        {
          "id": "joinByField",
          "options": {
            "byField": "task_name",
            "mode": "outer"
          }
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "Time 1": true,
              "Time 2": true,
              "Time 3": true,
              "Time 4": true,
              "Time 5": true,
              "Time 6": true,
              "Metric": true,
              "Metric 1": true,
              "Metric 2": true,
              "Metric 3": true,
              "Metric 4": true,
              "Metric 5": true,
              "Metric 6": true,
              "__name__": true,
              "__name__ 1": true,
              "__name__ 2": true,
              "__name__ 3": true,
              "__name__ 4": true,
              "__name__ 5": true,
              "__name__ 6": true,
              "instance": true,
              "instance 1": true,
              "instance 2": true,
              "instance 3": true,
              "instance 4": true,
              "instance 5": true,
              "instance 6": true,
              "job": true,
              "job 1": true,
              "job 2": true,
              "job 3": true,
              "job 4": true,
              "job 5": true,
              "job 6": true,
              "method": true,
              "method 1": true,
              "method 2": true,
              "method 3": true,
              "method 4": true,
              "method 5": true,
              "method 6": true
            },
            "renameByName": {
              "Value": "Task Status / 任务状态",
              "Value 1": "status_code",
              "Value 2": "timeout",
              "Value 3": "content",
              "Value 4": "json_path",
              "Value 5": "delay",
              "Value 6": "ssl_expiry",
              "Value #A": "Task Status / 任务状态",
              "Value #B": "status_code",
              "Value #C": "timeout",
              "Value #D": "content",
              "Value #E": "json_path",
              "Value #F": "delay",
              "Value #G": "ssl_expiry"
            },
            "indexByName": {
              "task_name": 0,
              "Task Status / 任务状态": 1,
              "status_code": 2,
              "timeout": 3,
              "content": 4,
              "json_path": 5,
              "delay": 6,
              "ssl_expiry": 7
            }
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "orange",
                "value": 1
              }
            ]
          },
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {
                  "text": "OK",
                  "color": "green"
                },
                "1": {
                  "text": "ALERT",
                  "color": "orange"
                }
              }
            },
            {
              "type": "special",
              "options": {
                "match": "null",
                "result": {
                  "text": "NoData",
                  "color": "gray"
                }
              }
            }
          ],
          "custom": {
            "align": "center",
            "cellOptions": {
              "type": "color-background"
            }
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "task_name"
            },
            "properties": [
              {
                "id": "mappings",
                "value": []
              },
              {
                "id": "custom.cellOptions",
                "value": {
                  "type": "auto"
                }
              },
              {
                "id": "custom.align",
                "value": "left"
              },
              {
                "id": "custom.width",
                "value": 320
              },
              {
                "id": "custom.wrapText",
                "value": true
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Task Status / 任务状态"
            },
            "properties": [
              {
                "id": "custom.width",
                "value": 170
              }
            ]
          },
          {
            "matcher": {
              "id": "byRegexp",
              "options": "^(Task Status / 任务状态|status_code|timeout|content|json_path|delay|ssl_expiry)$"
            },
            "properties": [
              {
                "id": "custom.width",
                "value": 100
              },
              {
                "id": "custom.align",
                "value": "center"
              }
            ]
          }
        ]
      },
      "options": {
        "showHeader": true,
        "cellHeight": "sm",
        "enablePagination": true,
        "sortBy": [
          {
            "displayName": "task_name",
            "desc": false
          }
        ],
        "footer": {
          "show": false,
          "reducer": [
            "sum"
          ],
          "countRows": false
        }
      }

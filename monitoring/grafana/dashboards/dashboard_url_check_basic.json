    {
      "id": 201,
      "type": "stat",
      "title": "当前异常任务数 / Active Alert Tasks",
      "description": "当前有任意告警项触发的任务数量。",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(clamp_max(url_check_status_code_alert + url_check_timeout_alert + url_check_content_alert + url_check_json_path_alert + url_check_delay_alert + url_check_ssl_expiry_alert, 1))"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "decimals": 0,
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "orange",
                "value": 1
              }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "colorMode": "background",

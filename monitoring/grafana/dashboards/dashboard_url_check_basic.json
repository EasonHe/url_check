      },
      "gridPos": {
        "x": 12,
        "y": 18,
        "w": 12,
        "h": 10
      },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(5, histogram_quantile(0.95, sum by (le, task_name) (rate(url_check_http_response_time_ms_bucket[5m]))))",
          "legendFormat": "{{task_name}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
